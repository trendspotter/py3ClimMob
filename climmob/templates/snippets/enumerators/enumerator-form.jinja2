<div class="form-group">
    <label class="col-sm-4 control-label">{{ _('User name') }}</label>
    <div class="col-sm-8"><input type="text" class="form-control" value="{{ dataworking['enum_id'] }}" id="enum_id" name="enum_id" placeholder="" {% if modify %}readonly{% endif %}></div>
</div>
<div class="form-group">
    <label class="col-sm-4 control-label">{{ _('Name') }}</label>
    <div class="col-sm-8"><input type="text" class="form-control" value="{{ dataworking['enum_name'] }}" id="enum_name" name="enum_name" placeholder=" "></div>
</div>
{% if not modify %}
    <div class="form-group">
        <label class="col-sm-4 control-label">{{ _('Password') }} </label>
        <div class="col-sm-8"><input type="password" class="form-control" value="" id="enum_password" name="enum_password" placeholder=" "></div>
    </div>
    <div class="form-group">
        <label class="col-sm-4 control-label">{{ _('Retype password') }} </label>
        <div class="col-sm-8"><input type="password" class="form-control" value="" id="enum_password_re" name="enum_password_re" placeholder=" "></div>
    </div>
{% else %}
    <div class="form-group">
        <label class="col-sm-4 control-label">{{ _('Can submit data') }}</label>
        <div class="col-sm-8"><input type="checkbox" id="ckb_modify_status" name="ckb_modify_status" data-on-color="success" data-off-color="danger" data-on-text="{{ _('YES') }}" data-off-text="{{ _('NO') }}" {% if dataworking['enum_active'] ==1 %}checked{% endif %}></div>
    </div>

{% endif %}



